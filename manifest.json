{
  "manifest_version": 2,

  "name": "Bookmarker",
  "description": "Bookmarks",
  "version": "1.2",
    
  "browser_action": {
      "default_title": "My Bookmarks",
      "default_icon": "icon.png",
      "default_popup": "index.html"
  },
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },
  "permissions":["tabs", "cookies", "storage", "webNavigation"]
  
}